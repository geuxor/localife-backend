{
  "env": {
    "node": true,
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "quotes": [
      "warn",
      "single"
    ],
    "no-octal": "off",
    "no-unused-vars": "warn"
  }
}